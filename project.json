{
  // Language version of C3.
  "langrev": "1",
  // Warnings used for all targets.
  "warnings": [ "no-unused" ],
  // Directories where C3 library files may be found.
  "dependency-search-paths": [ "lib" ],
  "dependencies": [ ],
  "authors": [ "EngineersBox" ],
  "version": "0.1.0",
  "sources": [ "src/**" ],
  "test-sources": [ "test/**" ],
  "c-sources": [ "csrc/**" ],
  "c-include-dirs": [ "external/PieceChain/include" ],
  // Build location, relative to project file.
  "build-dir": "build",
  // Output location, relative to project file.
  "output": "build",
  "targets": {
    "flow": {
      "type": "executable",
      "target": "macos-aarch64",
      "linked-libraries": ["PieceChain"],
      "linker-search-paths": ["external/PieceChain/"]
    },
  },
  "cpu": "generic",
  // Optimization: "O0", "O1", "O2", "O3", "O4", "O5", "Os", "Oz".
  "opt": "O0"
}
